#= require jquery.serialize-object
#= require recommender/main
#= require_tree ../templates/recommender
#= require_tree ./recommender/models
#= require_tree ./recommender/collections
#= require_tree ./recommender/views
#= require_tree ./recommender/routers

window.Recommender =
  Models: {}
  Collections: {}
  Views: {}
  Routers: {}
  initialize: -> 
    Recommender.Collections.Recommends::url = '/recommender/recommends'
    new Recommender.Routers.Recommends()
    Backbone.history.start()

$(document).ready ->
  Recommender.initialize()
